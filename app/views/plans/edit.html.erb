<%= form_with(model: @plan, local: true) do |form| %>

  <div class="housewok_item">
    <%= form.label :housework %>
    <%= form.collection_select :housework_id, Housework.all, :id, :title %>
  </div>

  <div class="time">
    <%= form.label :start_time %>
    <%= form.datetime_select :start_time %>
  </div>

  <div class="submit">
    <%= form.submit %>
  </div>
  
<% end %>
